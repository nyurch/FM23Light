		<record get_property="index" set_property="select_event" target="dui0"/>
		<record get_property="select_event" set_property="select_event" target="lstb"/>
		<record get_property="psrs" set_property="valu" target="box2"/>
		<record get_property="ind2" set_property="select_event" target="dui1"/>
		<record get_property="select_event" set_property="select_event" target="lst2"/>
