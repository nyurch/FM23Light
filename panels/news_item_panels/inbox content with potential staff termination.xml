		<widget class="text" id="stri" text="[%string#1]" auto_size="vertical"/>

		<container class="formatted_titled_box" id="box1" auto_size="vertical" appearance="boxes/inner/titled/paper" format="[%string#1]">
			<attachment_group class="vertical_arrange_autosize"/>

			<widget class="check_box" id="dui0" value="true" auto_size="all"/>

			<widget class="client_object_list_table" id="lstb" mode="fill_rows, stripe_rows, show_bullets, select_rows, dont_allow_empty_selection" auto_size_rows="true" auto_size="vertical">
				<record id="viewed_property_ids">
					<record id="person_person" width="-5" colour="black"/>
					<record id="psrm" width="-3"/>
					<record id="ptfe" width="-3"/>
				</record>
			</widget>
		</container>

		<container class="formatted_titled_box" id="box2" auto_size="vertical" appearance="boxes/inner/titled/paper" format="[%string#1]" hidden="true">
			<attachment_group class="vertical_arrange_autosize"/>

			<widget class="check_box" id="dui1" value="true" auto_size="all"/>

			<widget class="client_object_list_table" id="lst2" mode="fill_rows, stripe_rows, show_bullets, select_rows, dont_allow_empty_selection" auto_size_rows="true" auto_size="vertical">
				<record id="viewed_property_ids">
					<record id="person_person" width="-5"/>
					<record id="psrs" width="-3"/>
					<record id="ptf2" width="-3"/>
				</record>
			</widget>
		</container>
