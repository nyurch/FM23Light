		<container class="tabs" id="main" default_item="mpts" save_session_state="true">

			<container id="mpts" class="team_next_match_panel">
				<translation id="title" translation_id="308589" type="use" value="Next Match[COMMENT: Text on match preparation fixture tab]" />
				<record id="object_property">
					<integer id="get_property" value="objt" />
					<integer id="set_property" value="objt" />
				</record>
			</container>
			<container id="mppm" class="team_next_match_past_meetings_panel">
				<translation id="title" translation_id="308590" type="use" value="Past Meetings[COMMENT: Text on match preparation fixture tab]" />
				<record id="object_property">
					<integer id="get_property" value="objt" />
					<integer id="set_property" value="objt" />
				</record>
			</container>
			<container id="Mpsr">
				<translation id="title" translation_id="338086" type="use" value="Report[COMMENT: Text on match preparation fixture tab]" />

				<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0" gap="4" />
