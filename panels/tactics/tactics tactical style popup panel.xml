<panel width="500">
	<attachment_group class="vertical_arrange_autosize" vertical_gap="8" horizontal_inset="0" />

	<container class="main_contrast_box" id="ppcn" red_replacement="card bg darker" appearance="boxes/popup/standard/paper">
		<attachment_group class="horizontal_arrange_vertical_autosize" horizontal_layout="-1, 3, -1" horizontal_gap="8" horizontal_offset="8" vertical_inset="0" />

		<widget class="table" id="PrST" layout="-1" mode="fill_rows, stripe_rows" auto_size="all" navigation_container="true" fixed_row_height="25" row_height="29" sort_disabled="true">
			<attachment class="test_screen_size_attachment">
				<list id="properties">
					<record>
						<flags id="alignment" value="vertical"/>
						<integer id="min_value" value="0" />
						<integer id="max_value" value="700"/>
						<symbol id="set_property" value="auto_size_maximum_height" />
						<integer id="true_value" value="290" />
						<integer id="false_value" value="100000" />
						<boolean id="test_on_layout" value="true"/>
					</record>
				</list>
			</attachment>
			<list id="headings">
				<record alignment="left, centre_y">
					<flags id="colour" value="teal 900" />
					<flags id="font" value="title" />
					<translation id="text" translation_id="466515" type="use" value="Preset Styles[COMMENT: FR 23334; tactics panel; tactical styles popup; table heading for preset styles list]" />
				</record>
			</list>

			<list id="column_table_properties">
				<record index="0">
					<flags id="alignment" value="left, centre_y" />
					<integer id="left" value="4" />
				</record>
			</list>

			<list id="column_widget_properties">
				<record index="0">
					<flags id="alignment" value="left, centre_y" />
					<flags id="colour" value="black" />
				</record>
			</list>
		</widget>

		<widget class="vertical_divider">
			<layout class="stick_to_sides_attachment" alignment="vertical" inset="10" />
		</widget>

		<widget class="table" id="CuST" layout="-1, 25" mode="fill_rows, stripe_rows" auto_size="all" navigation_container="true" fixed_row_height="25" row_height="29">
			<list id="headings">
				<record alignment="left, centre_y" column_span="2">
					<flags id="colour" value="teal 900" />
					<flags id="font" value="title" />
					<translation id="text" translation_id="466516" type="use" value="Custom Styles[COMMENT: FR 23334; tactics panel; tactical styles popup; table heading for custom styles list]" />
				</record>
			</list>

			<list id="column_table_properties">
				<record index="0">
					<flags id="alignment" value="left, centre_y" />
					<integer id="left" value="0" />
				</record>
			</list>

			<list id="column_widget_properties">
				<record index="0">
					<flags id="alignment" value="left, centre_y" />
					<flags id="colour" value="black" />
				</record>
			</list>
		</widget>
	</container>

</panel>